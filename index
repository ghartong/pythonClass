        var winObjRef = null;
        var winObjRef2 = null;
        function tipsPop(strUrl,strWindowName){
            if(winObjRef == null || winObjRef.closed){
                try{
                    winObjRef2 = window.open('/signetintranet/tips/tipsCard.cfm','card');
                    winObjRef = window.open(strUrl,strWindowName);
                } catch(e){};
            }else{
                winObjRef.focus();
            };
        }

    </script>
